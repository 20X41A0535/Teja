pipeline{
agent any
stages
{
stage('check validate')
{
steps{
echo 'iam validating the code'
}
}
stages
{
stage('compile')
{
steps{
echo 'iam compiling the code'
}
}stages
{
stage(' install')
{
steps{
echo 'iam installing the code'
}
}stages
{
stage(' deploy')
{
steps{
echo 'iam deploying the code'
}
}
}
}
